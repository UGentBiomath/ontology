@prefix energysource:<https://ugentbiomath.github.io/ontology/energy.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

# --------------Classes -----------------------------------------
energysource:WaterConveyance a owl:Class . # Water Conveyance as an energy source
energysource:WaterTreatment a owl:Class . # Water Treatment as an energy source
energysource:WaterDistribution a owl:Class . # Water Distribution as an energy source
energysource:WasteWaterCollection a owl:Class . # Waste Water Collection as an energy source
energysource:WasteWaterTreatment a owl:Class . # Waste Water Treatment as an energy source
energysource:RecycledWasteWaterTreatment a owl:Class . # Recycled Waste Water Treatment as an energy source
energysource:RecycledWaterDistribution a owl:Class . # Recycled Water Distribution as an energy source
energysource:WasteWaterDischarge a owl:Class . # Waste Water Discharge as an energy source

energysource:GroundWaterPumping a owl:class ;
rdfs:subclassof energysource:Waterconveyance ;
rdfs:label "Ground Water Pumping (Water Coveyance)" .

energysource:SurfaceWaterPumping a owl:class ;
rdfs:subclassof energysource:Waterconveyance ;
rdfs:label "Surface Water Pumping (Water Conveyance)" .

energysource:PrimaryPumping a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "Primary Pumping (Water Treatment)" .

energysource:SecondaryPumping a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "Secondary Pumping (Water Treatment)" .

energysource:RO a owl:class;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "  RO (Water Treatment)" .

energysource:SWDesal a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label " Sea Water Desalination (Water Treatment) " .

energysource:MSFandMEDandMVC a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label " MSF,MED and MVC Desalination (Water Treatment)" .

energysource:DistributionPumping a owl:class ;
rdfs:subclassof energysource:WaterDistribution ;
rdfs:label " Distribution Pumping (Water Distribution)" .

energysource:Secondary a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Secondary waste water treatment" .

energysource:SecondarywithNitrification a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Secondary waste water treatment with Nitrification" .

energysource:TertiarywithTricklingFilter a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Tertiary waste water treatment with trickling filter " .

energysource:TertiarywithActivatedSludge a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Tertiary waste water treatment with activated sludge " .

#--------------------------Properties-------------------------

energysource:hassourcedischargelocation a owl:objectproperty ;
rdfs:domain energysource:GroundWaterPumping ;
rdfs:range energysource:hassourcedischargelocation ;
rdfs:label "has source discharge location " .

energysource:hasflowresistance a owl:objectproperty ;
rdfs:domain energysource:GroundWaterPumping ;
rdfs:range energysource:hasflowresistance ;
rdfs:label "has flow resistance " .






