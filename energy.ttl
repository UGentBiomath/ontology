@prefix energysource:<https://ugentbiomath.github.io/ontology/energy.ttl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.w3.org/2006/time#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix qudt: <http://qudt.org/schema/qudt/> .
@prefix unit: <http://qudt.org/vocab/unit/> .
@prefix quantity: <http://qudt.org/vocab/quantitykind/> .


# --------------Classes -----------------------------------------
energysource:EnergySourceProcess a owl:class ;
rdfs:label "Energy Source Process " .

energysource:hasEnergyConsumption a owl:ObjectProperty ;
rdfs:domain energysource:EnergySourceProcess ;
rdfs:range qudt:QuantityValue ;
rdfs:label "has energy consumption" .

energysource:WaterConveyance a owl:Class;
rdfs:subclassof energysource:EnergySourceProcess;
rdfs:label "Water Conveyance".

energysource:WaterTreatment a owl:Class ;
rdfs:subclass energysource:EnergySourceProcess;
rdfs:label " Water Treatment".

energysource:WaterDistribution a owl:Class ;
rdfs:subclassof energysource:EnergySourceProcess;
rdfs:label " Water Distribution".

energysource:WasteWaterCollection a owl:Class ;
rdfs:subclassof energysource:EnergySourceProcess;
rdfs:label " Water Distribution".

energysource:WasteWaterTreatment a owl:Class ;
rdfs:subclassof energysource:EnergysourceProcess;
rdfs:label " Waste Water Treatment".

energysource:RecycledWasteWaterTreatment a owl:Class ;
rdfs:subclassof energysource:EnergySourceProceess;
rdfs:label "Recycled Waste Water Treatment" .

energysource:RecycledWaterDistribution a owl:Class ;
rdfs:subclassof energysource:EnergySourceProceess;
rdfs:label "Recycled Water Distribution" .

energysource:WasteWaterDischarge a owl:Class ;
rdfs:subclassof energysource:EnergySourceProceess;
rdfs:label "Waste Water Discharge" .

energysource:GroundWaterPumping a owl:class ;
rdfs:subclassof energysource:Waterconveyance ;
rdfs:label "Ground Water Pumping (Water Conveyance)" .

energysource:SurfaceWaterPumping a owl:class ;
rdfs:subclassof energysource:Waterconveyance ;
rdfs:label "Surface Water Pumping (Water Conveyance)" .

energysource:PrimaryPumping a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "Primary Pumping (Water Treatment)" .

energysource:SecondaryPumping a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "Secondary Pumping (Water Treatment)" .

energysource:RO a owl:class;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label "  RO (Water Treatment)" .

energysource:SWDesal a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label " Sea Water Desalination (Water Treatment) " .

energysource:MSFandMEDandMVC a owl:class ;
rdfs:subclassof energysource:WaterTreatment ;
rdfs:label " MSF,MED and MVC Desalination (Water Treatment)" .

energysource:DistributionPumping a owl:class ;
rdfs:subclassof energysource:WaterDistribution ;
rdfs:label " Distribution Pumping (Water Distribution)" .

energysource:Secondary a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Secondary waste water treatment" .

energysource:SecondarywithNitrification a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Secondary waste water treatment with Nitrification" .

energysource:TertiarywithTricklingFilter a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Tertiary waste water treatment with trickling filter " .

energysource:TertiarywithActivatedSludge a owl:class ;
rdfs:subclassof energysource:WasteWaterTreatment ;
rdfs:label " Tertiary waste water treatment with activated sludge " .

#--------------------------Properties-------------------------

#--------Sub Properties of Ground Water Pumping---------------

energysource:hassourcedischargelocation a owl:objectproperty ;
rdfs:domain energysource:GroundWaterPumping ;
rdfs:range energysource:hassourcedischargelocation ;
rdfs:label "has source discharge location " .

energysource:hasflowresistance a owl:objectproperty ;
rdfs:domain energysource:GroundWaterPumping ;
rdfs:range energysource:hasflowresistance ;
rdfs:label "has flow resistance " .

energysource:GroundWaterPumping rdfs:subClassOf [
    a owl:Restriction ;
    owl:onProperty energysource:hasLiftDistance ;
    owl:someValuesFrom xsd:float
],
[
    a owl:Restriction ;
    owl:onProperty energysource:hasFlowVolume ;
    owl:someValuesFrom xsd:float
],
[
    a owl:Restriction ;
    owl:onProperty energysource:hasPressureRequirement ;
    owl:someValuesFrom xsd:float
],
[
    a owl:Restriction ;
    owl:onProperty energysource:hasTime ;
    owl:someValuesFrom xsd:float
] .

energysource:GroundWaterPumping
    rdfs:subClassOf [
        a owl:Restriction ;
        owl:onProperty energysource:hasEnergyConsumption ;
        owl:someValuesFrom [
            a rdfs:Datatype ;
            owl:onDatatype xsd:float ;
            owl:withRestrictions (
                [ xsd:minInclusive "0.14"^^xsd:float ]
                [ xsd:maxInclusive "0.69"^^xsd:float ]
            )
        ]
    ] .





