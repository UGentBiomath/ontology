#--------Properties ---------------
energysource:hasEnergyConsumption a owl:ObjectProperty ;
rdfs:domain energysource:EnergySourceProcess ;
rdfs:range qudt:QuantityValue ;
rdfs:label "Energy consumption" .

energysource:hasThermalEnergy a owl:ObjectProperty ;
    rdfs:domain energysource:EnergySourceProcess ;
    rdfs:range qudt:QuantityValue ;
    rdfs:subPropertyOf energysource:hasEnergyConsumption ;
    rdfs:label "Thermal energy consumption" .

energysource:hasElectricalEnergy a owl:ObjectProperty ;
    rdfs:domain energysource:EnergySourceProcess ;
    rdfs:range qudt:QuantityValue ;
    rdfs:subPropertyOf energysource:hasEnergyConsumption ;
    rdfs:label "Electrical energy consumption".

